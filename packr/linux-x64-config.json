{
    "platform": "linux64",
    "jdk": "linux-jdk",
    "executable": "Harmony",
    "classpath": [
        "target/Harmony.jar"
    ],
    "mainclass": "net.runelite.launcher.Launcher",
    "vmargs": [
       "-Drunelite.launcher.reflect=true",
       "-Xmx512m",
       "-Xss2m",
       "-XX:CompileThreshold=1500"
    ],
    "output": "native-linux-x86_64/Harmony.AppDir/",
    "resources": [
      "target/filtered-resources/app.desktop",
      "appimage/app.png"
    ]
}
